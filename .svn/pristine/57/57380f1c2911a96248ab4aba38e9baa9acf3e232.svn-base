package com.super_bits.modulosSB.Persistencia.Campo;

import com.super_bits.modulosSB.SBCore.InfoCampos.campo.Campo;

public class CampoMask extends Campo {
	
	public CampoMask(String pNome,String pMascara,String pRegex) {
		super(null,null);
		mascara=pMascara;
		regex=pRegex;
		
	}
	public String getMascara() {
		return mascara;
	}
	public void setMascara(String mascara) {
		this.mascara = mascara;
	}
	public String getRegex() {
		return regex;
	}
	public void setRegex(String regex) {
		this.regex = regex;
	}
	private static final long serialVersionUID = 5922741395304136043L;
	private String mascara;
	private String regex;
}
