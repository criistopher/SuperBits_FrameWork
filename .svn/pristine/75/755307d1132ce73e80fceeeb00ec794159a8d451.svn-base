package com.super_bits.modulosSB.webPaginas.JSFBeans.SB.chart;

import com.super_bits.modulosSB.Persistencia.Campo.CampoEsperado;
import com.super_bits.modulosSB.Persistencia.anotacoes.InfoCampo.TC;
import com.super_bits.modulosSB.Persistencia.registro.persistidos.EntidadeSimples;
import com.super_bits.modulosSB.SBCore.BeansInterface.charts.ItfBeanSimpleChart;

@SuppressWarnings({ "serial", "rawtypes" })
public class B_ChartSimples<T> extends EntidadeSimples implements ItfBeanSimpleChart{

	public B_ChartSimples() {
		super();
		adcionaCampoEsperado(new CampoEsperado(TC.CHART_LABEL));
		adcionaCampoEsperado(new CampoEsperado(TC.CHART_VALOR));
		adcionaCampoEsperado(new CampoEsperado(TC.CHART_CATEGORIA));
	}

	public String getLabel() {
		
	  return (String) GetValorByTipoCampoEsperado(TC.CHART_LABEL);
		
	}

	public Double getValor() {
	
		Double resposta=Double.valueOf(GetValorByTipoCampoEsperado(TC.CHART_VALOR).toString());
		return resposta;
	}

	public String getCategoria() {
		
		return (String) GetValorByTipoCampoEsperado(TC.CHART_CATEGORIA);
	}

}
